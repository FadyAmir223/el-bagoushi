generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String   @id @default(cuid())
  name        String   @unique
  description String
  price       Float
  image       String
  createdAt   DateTime @default(now())
  modifiedAt  DateTime @updatedAt
}
